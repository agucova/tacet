{
  "name": "@timing-oracle/js",
  "version": "0.1.0",
  "description": "Timing side-channel detection for Node.js/Bun",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./native": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    }
  },
  "napi": {
    "name": "timing-oracle",
    "constEnum": false,
    "dtsHeader": "// Native bindings for @timing-oracle/js\n// Generated by napi-rs - do not edit manually\n",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-pc-windows-msvc"
      ]
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.map"
  ],
  "scripts": {
    "build:native": "napi build --platform --release",
    "build:native:debug": "napi build --platform",
    "build:ts": "tsc",
    "build": "bun run build:native && bun run build:ts",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "example": "bun run examples/simple.ts",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts",
    "universal": "napi universal"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0",
    "@types/bun": "^1.0.0",
    "@types/node": "^25.0.10",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/agucova/timing-oracle"
  },
  "keywords": [
    "security",
    "cryptography",
    "timing",
    "side-channel",
    "constant-time"
  ],
  "optionalDependencies": {
    "@timing-oracle/js-darwin-arm64": "0.1.0",
    "@timing-oracle/js-darwin-x64": "0.1.0",
    "@timing-oracle/js-linux-arm64-gnu": "0.1.0",
    "@timing-oracle/js-linux-x64-gnu": "0.1.0",
    "@timing-oracle/js-win32-x64-msvc": "0.1.0"
  }
}
