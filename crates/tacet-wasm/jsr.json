{
  "name": "@tacet/js",
  "version": "0.2.3",
  "description": "Timing side-channel detection for JavaScript/TypeScript. Detect timing leaks in cryptographic code using statistical analysis.",
  "license": "MPL-2.0",
  "exports": "./dist/index.js",
  "publish": {
    "include": [
      "dist/**/*.js",
      "dist/**/*.d.ts",
      "dist/**/*.map",
      "pkg/*.js",
      "pkg/*.d.ts",
      "pkg/*.wasm",
      "README.md",
      "LICENSE",
      "logo.svg"
    ],
    "exclude": [
      "ts/",
      "src/",
      "node_modules/",
      "target/",
      "!dist/",
      "!pkg/"
    ]
  }
}
